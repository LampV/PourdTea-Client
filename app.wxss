@import "colorui/main.wxss";
@import "colorui/icon.wxss";
.poemlist_scrollview {
    width: 750rpx;
    padding: 0rpx 10rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: space-between;
    box-sizing: border-box;
    background-color: #fafafb;
}
.poems-container:before{
  position: absolute;
  background-color: #EBEBEB;
  position: fixed;
  height: 2rpx;
  width: 732rpx;
  content: " ";
}
.color-grey{
  color: #666666;
}
.color-orange{
  color: #cd950c;
}
.color-ricewhite{
  color: #fafafb;
}