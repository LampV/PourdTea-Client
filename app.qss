.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}
.poem-container{
  background-color: #FFFFFF;
  margin: 14rpx 0;
  padding: 6rpx 6rpx;
  border: solid 2rpx #B0B0B0;
  border-radius: 10rpx;
}